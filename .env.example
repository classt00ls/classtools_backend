# Base de datos principal para NestJS/TypeORM
DB_HOST=localghost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=classtools
DB_SSL=false

# Base de datos para PGVector/Embeddings (puede ser la misma o diferente)
PGVECTOR_HOST=localghost
PGVECTOR_PORT=5432
PGVECTOR_USER=postgres
PGVECTOR_PASSWORD=postgres
PGVECTOR_DB=classtools
PGVECTOR_SSL=false

# Configuración de la tabla de embeddings
PGVECTOR_TABLE=embeddings
PGVECTOR_COL_ID=id
PGVECTOR_COL_CONTENT=content
PGVECTOR_COL_METADATA=metadata
PGVECTOR_COL_VECTOR=embedding

# Configuración de Ollama para generación de embeddings
OLLAMA_BASE_URL=http://localghost:11434
OLLAMA_EMBEDDINGS_MODEL=nomic-embed-text

# Configuración para generación de respuestas de LLM
OLLAMA_LLM_MODEL=llama3
OLLAMA_DEFAULT_TEMPERATURE=0.7
OLLAMA_DEFAULT_LIMIT=5 